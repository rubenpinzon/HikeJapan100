body
  margin 0
  background-color #78d0e4
  font-family 'Yanone Kaffeesatz', sans-serif
  font-weight 200
  font-size 17px

#map-holder
  width 100vw
  height 100vh

svg
  rect
    fill #fafff8

.country
  fill #d0d0d0
  stroke #2A2C39
  stroke-width 1

.country-on
  fill #4B5358

.countryLabel
  display none

.countryName
  fill #FFFAFF

.countryLabelBg
  fill #30BCED